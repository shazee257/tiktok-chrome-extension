{
    "name": "tiktok extension",
    "version": "1.0.0",
    "description": "tiktok extension",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "https://www.tiktok.com/*",
        "https://www.tiktok.com/"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "tiktok extension"
    },
    "content_scripts": [
        {
            "matches": ["https://www.tiktok.com/*"],
            "js": ["content.js"]
        }
    ]

}



    

